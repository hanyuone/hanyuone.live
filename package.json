{
  "name": "hanyuone.live",
  "version": "0.6.0",
  "description": "A personal website written in Rust.",
  "main": "index.js",
  "scripts": {
    "dev:md": "cargo watch -w markdown/src -w blogs/md -x \"run -p markdown\"",
    "dev:site": "cd website && cargo leptos watch",
    "dev": "concurrently -c auto \"pnpm dev:md\" \"pnpm dev:site\"",
    "build:md": "cargo run -p markdown",
    "build:site": "cd website && cargo leptos build --release",
    "build": "pnpm build:md && pnpm build:site"
  },
  "keywords": [],
  "author": "hanyuone",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
